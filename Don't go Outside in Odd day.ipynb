   "source": [
    "l = [1,2,3,4,5,6,7,8,9]\n",
    "even = 0\n",
    "odd = 0\n",
    "for i in l:\n",
    "    if i%2==0:\n",
    "        even+= 1\n",
    "    else:\n",
    "        odd+= 1\n",
    "print(\"Number of Even Numbers are:\",even)\n",
    "print(\"Number of Odd Numbers are:\",odd)"
