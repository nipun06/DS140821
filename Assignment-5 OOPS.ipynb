    "class square_power:\n",
    "    def __init__(self,a,n):\n",
    "        self.a=a\n",
    "        self.n=n\n",
    "        self.ans=0\n",
    "    def square(self):\n",
    "        self.ans=pow(self.a,self.n)\n",
    "        print(self.ans)"
   ]
